<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="500.0" prefWidth="894.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gameproject.FirstPageController">
   <children>
      <ImageView fitHeight="526.0" fitWidth="898.0" layoutX="-3.0" layoutY="-5.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../Maps/llogo.jpg" />
         </image>
      </ImageView>
      <Button fx:id="signinBT" layoutX="380.0" layoutY="363.0" lineSpacing="10.0" mnemonicParsing="false" onAction="#signIn" prefHeight="52.0" prefWidth="121.0" text="SignIn">
         <font>
            <Font size="25.0" />
         </font>
      </Button>
      <Button fx:id="signUpBT" layoutX="378.0" layoutY="304.0" mnemonicParsing="false" onAction="#signUp" prefHeight="52.0" prefWidth="127.0" text="SignUp">
         <font>
            <Font size="25.0" />
         </font>
      </Button>
      <Button fx:id="exitBT" layoutX="380.0" layoutY="423.0" mnemonicParsing="false" onAction="#exit" prefHeight="52.0" prefWidth="121.0" text="Exit">
         <font>
            <Font size="25.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
