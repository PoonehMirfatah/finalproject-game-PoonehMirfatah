<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="700.0" prefWidth="800.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gameproject.FirstPageController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <ImageView fitHeight="319.0" fitWidth="553.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../Photos/LOGO.jpg" />
         </image>
      </ImageView>
      <Button fx:id="signinBT" lineSpacing="10.0" mnemonicParsing="false" onAction="#signIn" prefHeight="65.0" prefWidth="135.0" text="SignIn">
         <font>
            <Font size="30.0" />
         </font>
      </Button>
      <Button fx:id="signUpBT" mnemonicParsing="false" onAction="#signUp" text="SignUp">
         <font>
            <Font size="30.0" />
         </font>
      </Button>
      <Button fx:id="exitBT" mnemonicParsing="false" onAction="#exit" prefHeight="65.0" prefWidth="135.0" text="Exit">
         <font>
            <Font size="30.0" />
         </font>
      </Button>
   </children>
</VBox>
