<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gameproject.SignUpPageController">
    <children>
        <Label layoutX="252.0" layoutY="78.0" text="SignUP">
            <font>
                <Font size="30.0" />
            </font>
        </Label>
        <TextField fx:id="usernameTF" layoutX="261.0" layoutY="202.0" prefHeight="37.0" prefWidth="160.0" />
        <TextField fx:id="passwordTF" layoutX="262.0" layoutY="272.0" prefHeight="37.0" prefWidth="160.0" />
        <Label layoutX="130.0" layoutY="202.0" text="UserName">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <Label layoutX="137.0" layoutY="276.0" text="Password">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
      <Button fx:id="confirmBT" layoutX="439.0" layoutY="342.0" mnemonicParsing="false" onAction="#signUp" text="Confirm">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
